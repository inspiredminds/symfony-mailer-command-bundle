{
    "name": "inspiredminds/symfony-mailer-command-bundle",
    "description": "Implements a mailer:send command for the Symfony Mailer within the Symfony Framework.",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "homepage": "https://github.com/inspiredminds/symfony-mailer-command-bundle",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
	"support": {
		"issues": "https://github.com/inspiredminds/symfony-mailer-command-bundle/issues",
		"source": "https://github.com/inspiredminds/symfony-mailer-command-bundle"
	},
	"funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/console": "^5.4 || ^6.1",
        "symfony/mime": "^5.4 || ^6.1",
        "symfony/mailer": "^5.4 || ^6.1",
        "symfony/http-kernel": "^5.4 || ^6.1"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\SymfonyMailerCommandBundle\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16 || ^3.0"
    },
    "config": {
        "sort-packages": true
    }
}
